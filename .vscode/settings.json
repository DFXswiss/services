{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.sortMembers": true
  },
  "git.ignoreLimitWarning": true,
  "cSpell.words": [
    "alby",
    "arbitrum",
    "bahnhofstrasse",
    "cardano",
    "defichain",
    "iban",
    "ibantools",
    "noreferrer",
    "olkilul",
    "onlineid",
    "rabby",
    "refcode",
    "sepa",
    "tailwindcss",
    "videoid"
  ],
  "files.exclude": {
    "node_modules": true
  },
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#072440",
    "titleBar.activeForeground": "#FFF"
  }
}
